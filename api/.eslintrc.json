{
    "extends": [
        "@rocketseat/eslint-config/node"
    ],
    "rules": {
        "camelcase": "off",
        "react-hooks/exhaustive-deps": "off",
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "singleQuote": true,
                "tabWidth": 2,
                "printWidth": 80
            }
        ]
    }
}