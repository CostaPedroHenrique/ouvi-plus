{
    "extends": [
        "@rocketseat/eslint-config/node"
    ],
    "rules": {
        "camelcase": "off",
        "react-hooks/exhaustive-deps": "off",
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "none",
                "semi": true,
                "tabWidth": 2,
                "singleQuote": false,
                "bracketSpacing": true,
                "jsxBracketSameLine": true,
                "useTabs": false
            }
        ]
    }
}